  <example id="lpcxpresso812max_led_blinky" name="led_blinky" category="demo_apps" dependency="platform.drivers.lpc_gpio.LPC812 platform.drivers.common.LPC812 platform.drivers.clock.LPC812 platform.drivers.power_no_lib.LPC812 platform.drivers.reset.LPC812 platform.drivers.swm.LPC812 platform.drivers.syscon.LPC812 device.LPC812_CMSIS.LPC812 component.miniusart_adapter.LPC812 device.LPC812_startup.LPC812 platform.drivers.lpc_miniusart.LPC812 platform.utilities.assert_lite.LPC812 utility.debug_console_lite.LPC812 platform.drivers.lpc_iocon_lite.LPC812 platform.Include_core_cm0plus.LPC812 platform.Include_common.LPC812 platform.Include_dsp.LPC812 platform.utilities.misc_utilities.LPC812 platform.drivers.swm_connections.LPC812 platform.drivers.syscon_connections.LPC812">
